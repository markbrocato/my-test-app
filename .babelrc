{
  "presets": [
    ["env", {
      "targets": {
        "browsers": "> 1%",
        "uglify": true
      },
      "useBuiltIns": "usage"
    }],
    "stage-0",
    "react"
  ],
  "env": {
    "production": {
      "presets": ["minify"]
    }
  },
  "plugins": [
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }],
    "transform-async-to-generator",
    "transform-decorators-legacy",
    "transform-object-rest-spread",
    "transform-class-properties"
  ]
}
